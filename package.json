{
  "name": "@libria/gridstack",
  "version": "0.0.1",
  "description": "Angular 6 wrapper for gridstack.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/pfms84/lb-gridstack"
  },
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "copy-readme": "copy README.md lib",
    "update-version-patch": "cd src/app/gridstack && npm version patch",
    "update-version-prerelease": "cd src/app/gridstack && npm version prerelease",
    "packagr": "ng-packagr -p ./src/app/gridstack/package.json && npm run copy-readme && cd tarball && npm pack ../lib && cd..",
    "packagr-prerelease": "npm run update-version-prerelease && npm run packagr",
    "packagr-patch": "npm run update-version-patch && npm run packagr",
    "publish": "npm publish lib --access=public"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^6.1.2",
    "@angular/common": "^6.1.2",
    "@angular/compiler": "^6.1.2",
    "@angular/core": "^6.1.2",
    "@angular/forms": "^6.1.2",
    "@angular/http": "^6.1.2",
    "@angular/platform-browser": "^6.1.2",
    "@angular/platform-browser-dynamic": "^6.1.2",
    "@angular/router": "^6.1.2",
    "core-js": "^2.4.1",
    "gridstack": "^0.3.0",
    "jquery": "^2.2.0",
    "jquery-ui-dist": "^1.12.1",
    "lodash": "^4.17.11",
    "rxjs": "^6.2.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular/cli": "~7.0.5",
    "@angular/compiler-cli": "^6.1.2",
    "@angular/language-service": "^6.1.2",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/jquery": "^3.2.18",
    "@types/lodash": "^4.14.92",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^1.6.0",
    "protractor": "^5.4.1",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "~2.9.2"
  }
}
